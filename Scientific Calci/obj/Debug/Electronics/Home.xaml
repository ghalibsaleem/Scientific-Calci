<Page
    x:Class="Scientific_Calci.Electronics.Home"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Scientific_Calci.Electronics"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel>
            <Image Source="Assets/Constant/alpha.png">

            </Image>
            <StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="Series Resistance : " FontSize="20" Margin="10"/>
                    <TextBox x:Name="series" Text="R1:R2:R3. . . . . . ." Margin="10" VerticalAlignment="Stretch"/>
                </StackPanel>
            </StackPanel>
            <StackPanel>
                <StackPanel Orientation="Horizontal">
                    <TextBlock Text="Parallel Resistance : " FontSize="20" Margin="10"/>
                    <TextBox x:Name="parallel" Text="R1:R2:R3. . . . . . ." Margin="10" VerticalAlignment="Stretch"/>
                </StackPanel>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>

